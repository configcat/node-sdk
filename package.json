{
  "name": "configcat-node",
  "version": "9.1.1",
  "description": "Official ConfigCat SDK to help you access your feature flags from a Node.js application.",
  "main": "lib/client.js",
  "types": "lib/client.d.ts",
  "scripts": {
    "test": "cross-env NODE_EXTRA_CA_CERTS=./test/cert/testCA.pem TS_NODE_PROJECT=./tsconfig.mocha.json nyc mocha --require ts-node/register 'test/**/*.ts' --exit --timeout 30000",
    "build": "tsc -p tsconfig.build.cjs.json && gulp",
    "prepare": "npm run build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "configcat",
    "config",
    "configuration",
    "remote configuration",
    "configcat client",
    "feature flags",
    "feature toggle",
    "feature switch",
    "canary release",
    "soft launch",
    "release strategy"
  ],
  "author": "ConfigCat",
  "license": "MIT",
  "homepage": "https://configcat.com",
  "engines": {
    "node": ">=14"
  },
  "engine-strict": true,
  "dependencies": {
    "configcat-common": "^7.0.1",
    "tslib": "^2.4.1",
    "tunnel": "0.0.6"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/tunnel": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "del": "^7.0.0",
    "eslint": "^8.34.0",
    "eslint-plugin-import": "^2.27.5",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.1.3",
    "gulp-typescript": "^6.0.0-alpha.1",
    "mocha": "^10.2.0",
    "mockttp": "^3.4.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "overrides": {
    "chokidar": "3.5.3",
    "glob-parent": "5.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/configcat/node-sdk"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html",
      "json",
      "lcov"
    ],
    "all": true,
    "include": [
      "src"
    ]
  }
}
